<script>(function(d, s, id) {
    var langForfacebook = "ar_AR";
    var myL = document.cookie;
    if( myL.indexOf("lang=ar") != -1 ){
        ///////This Lang is Arabic
        langForfacebook = "ar_AR";
    }
    else{
        ///////This Lang is English
        langForfacebook = "en_EN";
    }
  var js, fjs = d.getElementsByTagName(s)[0];
  if (d.getElementById(id)) return;
  js = d.createElement(s); js.id = id;
  js.src = 'https://connect.facebook.net/'+langForfacebook+'/sdk.js#xfbml=1&version=v2.11';
  fjs.parentNode.insertBefore(js, fjs);
}(document, 'script', 'facebook-jssdk'));</script>
