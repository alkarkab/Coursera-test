<script>
    $(document).ready(function(){

        var islogged = '';
        if(islogged!=''){ 
            $('#regNow').hide();
            $('#forReg').hide();
        }

        var myL = document.cookie;
        if( myL.indexOf("lang=ar") != -1 ){
            ///////This Lang is Arabic
            $('#basic-words-en').hide();
            $('#tips-en').hide();
        }
        else{
            ///////This Lang is English
            $('#basic-words-ar').hide();
            $('#tips-ar').hide();
        }
    });
</script>

