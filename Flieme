    <script>
        $(document).ready(function(){
            var keynow = '';
            
            $('input , textarea').keydown(function(e){
                
                if(e.which==13){ 
                    if( $(this).prop("tagName")=="INPUT" ) e.preventDefault();
                };
                
                // # $ F5 ; ' " / \ | = + * { } ? [ ] 
                if( e.key=='#' || e.key=='$' || e.which==116 || e.key==';' || e.which==187 || e.which==106 || e.which==107 || e.which==111 || e.which==220 || e.key=="'" || e.key=='"' || (e.shiftKey && e.which==191) || (e.shiftKey && e.which==219) || (e.shiftKey && e.which==221)  || e.key=='<' || e.key=='>' || (e.key=='[') || (e.key==']') || e.key=='\(' || e.key=='\)' ) {
                    e.preventDefault();
                    $('#temperr').remove();
                    $(this).after('<div dir="rtl" id="temperr" style="font-weight:bold;font-size:16px;background-color:#fff;color:red;width100%;border-radius:7px;">رجاء لا تستخدم ' + e.key + '</div>');
                }
                else{ $('#temperr').remove(); }
                
                //$(this).val( e.which ) //Show Key
            });
            
            
            $('input , textarea').bind("paste", function(e){
                e.preventDefault();
                var pastedData = e.originalEvent.clipboardData.getData('text');
                pastedData = pastedData.replace(/;/g,'').replace(/'/g,'').replace(/"/g,'').replace(/\//g,'').replace(/\\/g,'').replace(/\|/g,'').replace(/\=/g,'').replace(/\+/g,'').replace(/\*/g,'').replace(/\{/g,'').replace(/\}/g,'').replace(/\?/g,'').replace(/\؟/g,'').replace(/\[/g,'').replace(/\]/g,'').replace(/\$/g,'').replace(/\#/g,'');
                $(this).val(pastedData);
            });
            
        });
    </script>
    
